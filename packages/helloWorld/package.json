{
  "name": "@comp-test/hello-world",
  "version": "1.0.0",
  "description": "Simple Hello World component built with Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hello-world/hello-world.esm.js",
  "exports": {
    ".": {
      "import": "./dist/hello-world/hello-world.esm.js",
      "require": "./dist/hello-world/hello-world.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./hello-world": {
      "import": "./dist/components/hello-world.js",
      "types": "./dist/components/hello-world.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./react": {
      "import": "./bindings/react/index.js",
      "types": "./bindings/react/index.d.ts"
    },
    "./vue": {
      "import": "./bindings/vue/index.js",
      "types": "./bindings/vue/index.d.ts"
    },
    "./angular": {
      "import": "./bindings/angular/index.js",
      "types": "./bindings/angular/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/",
    "bindings/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "stencil",
    "web-components",
    "hello-world",
    "ui",
    "component"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@stencil/vue-output-target": "^0.11.8",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/html-vite": "^7.6.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0",
    "storybook": "^7.6.10",
    "typescript": "^5.3.0"
  }
}
